import java.util.*;
public class Antonandcurrencyyoualknow {

	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		String s=sc.nextLine();
		int l=s.length();
		int a=0,k=0;
		int arr[]=new int[l];
		for(int i=0;i<l;i++)
		{
			char c=s.charAt(i);
			String s1=String.valueOf(c);
			int i1=Integer.parseInt(s1);
			arr[i]=i1;
		}
		for(int i=l-1;i>0;i--)
		{
			if(arr[i]%2!=0)
			{
				int n=arr[i];
				if(arr[i-1]%2==0)
				{
					arr[i]=arr[i-1];
					arr[i-1]=n;
					k=1;
				}
			}
				
		}
		if(k!=0)
			for(int i=0;i<l;i++) {
			System.out.print(arr[i]);
			if(i==a-1)
			System.out.println();
			}
			else
				System.out.println("-1");
		
	}

}
