package package7;

public class Class007 {
	public static int maximum(int a,int b){
		if(a<b)
			return b;
		else
			return a;
	}
	public static int minimum(int a,int b){
		if(a<b)
			return a;
		else
			return b;
	}
	public static int subtract (int c,int d){
		int a=c;
		int b=d;
		int mx=maximum(a,b);
		int min=minimum(a,b);
		int sub=mx-min;
		return sub;
	}
	public static int division (int c,int d){
		int a=c;
		int b=d;
		int mx=maximum(a,b);
		int min=minimum(a,b);
		int div=mx/min;
		return div;
	}
	
	
	public static void main(String [] arg){
		int a=88;
		int b=90;
		System.out.println("Subtract " +a+ " - "+b+"="+subtract(a,b));
		System.out.println(division(a,b));
		
	}

}
